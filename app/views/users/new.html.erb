<h1>Register New User</h1>

<%= render partial: "shared/landing_header" %><br><br>

<%= form_with url: "/users", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br><br>
  <%= f.submit "Register" %>
<% end %>