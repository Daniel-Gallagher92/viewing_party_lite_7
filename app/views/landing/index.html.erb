<body>
  <div class="content">
    <center><table>
      <h1> Viewing Party </h1><hr>

      <div id="new_user">
        <%= button_to "Create New User", register_path, method: :get %>
      </div>

      <ul id='existing_users'>
        <h3> Existing Users </h3>
        <% @users.each do |user| %>
          <%= link_to user.email, user_path(user.id)  %><br><br>
        <% end %>
        </ul>
    </center></table>
  </div>
</body>